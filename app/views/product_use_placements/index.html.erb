<h1>Listing product_use_placements</h1>

<table>
  <tr>
  </tr>

<% for product_use_placement in @product_use_placements %>
  <tr>
		<td><%= product_use_placement.name %></td>
		<td><%= product_use_placement.percentage %></td>
		<td><%= product_use_placement.price %></td>
    <td><%= link_to 'Edit', edit_product_use_placement_path(product_use_placement) %></td>
    <td><%= link_to 'Destroy', product_use_placement, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

