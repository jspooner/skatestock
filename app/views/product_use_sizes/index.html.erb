<h1>Listing product_use_sizes</h1>

<table>
  <tr>
  </tr>

<% for product_use_size in @product_use_sizes %>
  <tr>
		<td><%= product_use_size.name %></td>
		<td><%= product_use_size.percentage %></td>
		<td><%= product_use_size.price %></td>
    <td><%= link_to 'Edit', edit_product_use_size_path(product_use_size) %></td>
    <td><%= link_to 'Destroy', product_use_size, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_use_size', new_product_use_size_path %>
