<h1>Listing product_use_industries</h1>

<table>
  <tr>
  </tr>

<% for product_use_industry in @product_use_industries %>
  <tr class="<%= cycle "odd", "even" %>">
		<td><%= product_use_industry.name %></td>
		<td><%= product_use_industry.percentage %></td>
		<td><%= product_use_industry.price %></td>
    <td><%= link_to 'Edit', edit_product_use_industry_path(product_use_industry) %></td>
    <td><%= link_to 'Destroy', product_use_industry, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New product_use_industry', new_product_use_industry_path %>
