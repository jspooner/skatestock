<h1>Listing price_option_names</h1>

<table>
  <tr>
  </tr>

<% for price_option_name in @price_option_names %>
  <tr>
		<td><%= price_option_name.name %></td>
    <td><%= link_to 'Show', price_option_name %></td>
    <td><%= link_to 'Edit', edit_price_option_name_path(price_option_name) %></td>
    <td><%= link_to 'Destroy', price_option_name, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

