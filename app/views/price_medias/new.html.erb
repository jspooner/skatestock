<h1>New media for <%= @price_usage.name %></h1>

<% form_for(@price_media) do |f| %>
  <%= f.error_messages %>
	<%= f.hidden_field :price_usage_id %>
	<p>
		<%= f.label :name %>
		<%= f.text_field :name %>
	</p>
	<p>
		<%= f.label :price %>
		<%= f.text_field :price %>
	</p>
	<p>
		<%= f.label :percentage %>
		<%= f.text_field :percentage %>
	</p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', price_usages_path %>
